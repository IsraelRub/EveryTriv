{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "proseWrap": "preserve",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "^react-(.*)$",
    "^(?!(@shared|@config|@internal|@features|@common|@/|react$|react-|\\.)).*$",
    "",
    "^@shared",
    "",
    "^@config",
    "^@internal",
    "^@features",
    "^@common",
    "",
    "^@/constants",
    "^@/components",
    "^@/hooks",
    "^@/services",
    "^@/types",
    "^@/utils",
    "^@/redux",
    "^@/",
    "^[./]"
  ],
  "importOrderCaseSensitive": false,
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
