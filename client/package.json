{
  "name": "everytriv-client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "start": "vite",
    "start:local": "copy .env.local .env & vite",
    "start:prod": "copy .env.prod .env & vite",
    "clean": "rimraf dist dist-temp node_modules/.cache node_modules/.vite .vite",
    "build": "pnpm run clean && tsc && vite build",
    "build:local": "copy .env.local .env & pnpm run build",
    "build:prod": "copy .env.prod .env & pnpm run build",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json}",
    "preview": "vite preview",
    "serve": "vite preview --port 3000",
    "serve:prod": "pnpm run build && npx serve dist -p 3000",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@react-spring/web": "^10.0.1",
    "@reduxjs/toolkit": "^2.2.7",
    "@tanstack/react-query": "^5.28.4",
    "@tanstack/react-query-devtools": "^5.85.0",
    "@use-gesture/react": "^10.3.1",


    "clsx": "^2.1.1",
    "everytriv-shared": "workspace:*",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.536.0",
    "react": "^18.2.0",

    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.30.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.10"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.41",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vitest": "^1.3.1"
  }
}
