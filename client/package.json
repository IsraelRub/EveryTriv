{
  "name": "everytriv-client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "start": "vite",
    "start:dev": "vite",
    "start:prod": "echo Use 'pnpm run start:prod' from root directory to start with Docker",
    "start:prod:local": "copy .env.prod .env & vite",
    "clean": "rimraf dist dist-temp node_modules/.cache node_modules/.vite .vite",
    "build": "pnpm run clean && tsc && vite build",
    "build:prod": "copy .env.prod .env & pnpm run build",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --config ../tools/.prettierrc --ignore-path ../tools/.prettierignore --write src/**/*.{ts,tsx,js,jsx,json}",
    "format:check": "prettier --config ../tools/.prettierrc --ignore-path ../tools/.prettierignore --check src/**/*.{ts,tsx,js,jsx,json}",
    "preview": "vite preview",
    "serve": "vite preview --port 3000",
    "serve:prod": "pnpm run build && npx serve dist -p 3000"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@reduxjs/toolkit": "^2.2.7",
    "@tanstack/react-query": "^5.28.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.536.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.61.1",
    "react-icons": "^5.5.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.30.1",
    "recharts": "^2.12.7",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^22.16.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "postcss": "^8.4.41",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  }
}
