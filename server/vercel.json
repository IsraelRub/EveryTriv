{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/trivia/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/user/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/points/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/game-history/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/payment/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/main.ts": {
      "maxDuration": 30
    }
  }
}
